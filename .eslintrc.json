{
  "rules": {
    "indent": [
      2,
      4
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "windows"
    ],
    "semi": [
      2,
      "always"
    ]
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended"],
  "plugins": [
  ],
  "parserOptions": {
    "ecmaVersion": 6
  }
}
